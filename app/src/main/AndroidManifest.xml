<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.techside.techsidecalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TECHSIDECALCULATOR">
        <activity
            android:name=".SumaActivity"
            android:exported="false" />
        <activity
            android:name=".MultiplicationActivity"
            android:exported="false" />
        <activity
            android:name=".RestaActivity"
            android:exported="false" />
        <activity
            android:name=".KelvinActivity"
            android:exported="false" />
        <activity
            android:name=".FactorialActivity"
            android:exported="false" />
        <activity
            android:name=".DivisionActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>